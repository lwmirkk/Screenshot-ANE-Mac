			<applicationDeployment>
				<nativeLibrary>SampleANE3.framework</nativeLibrary>
				<initializer>initializer</initializer>
				<finalizer>finalizer</finalizer>
			</applicationDeployment> 
		</platform>
	</platforms> 
	
</extension>
